{% extends 'base.html.twig' %}

{% block title %}AboutUs{% endblock %}

{% block body %}
    <h1>AboutUs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ about_u.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ about_u.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ about_u.description }}</td>
            </tr>
            <tr>
                <th>Features</th>
                <td>{{ about_u.features ? about_u.features|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>ContactNumber</th>
                <td>{{ about_u.contactNumber }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ about_u.image }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ about_u.Email }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ about_u.Address }}</td>
            </tr>
            <tr>
                <th>Links</th>
                <td>{{ about_u.Links ? about_u.Links|json_encode : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_administrator_about_us_index') }}">back to list</a>

    <a href="{{ path('app_administrator_about_us_edit', {'id': about_u.id}) }}">edit</a>

    {{ include('administrator/about_us/_delete_form.html.twig') }}
