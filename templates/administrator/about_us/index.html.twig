<!DOCTYPE html>

<title>AboutUs index</title>

{% block body %}
    <h1>AboutUs index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Description</th>
                <th>Features</th>
                <th>ContactNumber</th>
                <th>Image</th>
                <th>Email</th>
                <th>Address</th>
                <th>Links</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for about_u in aboutuses %}
            <tr>
                <td>{{ about_u.id }}</td>
                <td>{{ about_u.title }}</td>
                <td>{{ about_u.description }}</td>
                <td>{{ about_u.features ? about_u.features|json_encode : '' }}</td>
                <td>{{ about_u.contactNumber }}</td>
                <td>{{ about_u.image }}</td>
                <td>{{ about_u.Email }}</td>
                <td>{{ about_u.Address }}</td>
                <td>{{ about_u.Links ? about_u.Links|json_encode : '' }}</td>
                <td>
                    <a href="{{ path('app_administrator_about_us_show', {'id': about_u.id}) }}">show</a>
                    <a href="{{ path('app_administrator_about_us_edit', {'id': about_u.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_administrator_about_us_new') }}">Create new</a>
{% endblock %}
